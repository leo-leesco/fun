let rec ack = fun m -> fun n ->
	ifzero m
		then (n+1)
		else (
			ifzero n
				then (ack (m-1) 1)
				else (ack (m-1) (ack m (n-1)))
		)
in
let _ = print (ack 0 0) in
let _ = print (ack 1 0) in
let _ = print (ack 0 1) in
let _ = print (ack 1 1) in
let _ = print (ack 2 2) in
let _ = print (ack 3 3) in
let _ = print (ack 2 3) in
let _ = print (ack 3 2) in

